import math
sum=0
n=input("Enter the number=")
b=len(n)
for i in range (0,b):
    if(n[i]=='0' or n[i]=='1'):
        print(end='')
    else:
        print("You have entered a wrong binary code")
        break
for i in range (0,b):
    d=math.pow(2,b-1-i)*int(n[i])
    sum=sum+d
print("Decimal=",sum)
    
